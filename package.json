{
  "name": "react-test-shop-server",
  "description": "Server for React Test Shop",
  "version": "1.0.0",
  "author": "Naresh Bhatia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nareshbhatia/react-test-shop-server.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:debug": "ndb ts-node src/index.ts",
    "start:prod": "node dist",
    "dev": "nodemon --watch src -e ts --exec yarn start",
    "build": "tsc",
    "test": "jest",
    "format": "prettier --write README.md 'src/**/*.ts'"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.5",
    "@types/uuid": "^8.3.1",
    "babel-jest": "^27.3.1",
    "jest": "^27.3.1",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
