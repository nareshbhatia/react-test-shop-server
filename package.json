{
  "name": "react-test-shop-server",
  "description": "Server for React Test Shop",
  "version": "1.0.0",
  "author": "Naresh Bhatia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nareshbhatia/react-test-shop-server.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:debug": "ndb ts-node src/index.ts",
    "start:json-server": "json-server --port 9090 --no-cors --watch json-server/db.json",
    "start:prod": "node dist",
    "dev": "nodemon --watch src -e ts --exec yarn start",
    "build": "tsc",
    "format": "prettier --write README.md 'src/**/*.ts'"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^16.10.3",
    "@types/uuid": "^8.3.1",
    "nodemon": "^2.0.13",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
